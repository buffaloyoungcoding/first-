#include<bits/stdc++.h>
using namespace std;

int main()
{
	int n, q;
	cin >> n >> q;
	vector< vector<int> > vec(n);
	for(int i = 0; i < n; i++){
		int size, tmp;
		cin >> size;
		for(int j = 0; j < size; j++){
			cin >> tmp;
			vec[i].push_back(tmp);
		}
	}
	while(q--){
		int a, b;
		cin >> a >> b;
		cout << vec[a][b] << endl;
	}

return 0;
}
